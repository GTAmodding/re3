{
	"version": "2.0.0",
	"tasks": 
	[
		{
			"label": "generateVS2017",
			"type": "shell",
			"command": "${workspaceRoot}/MakeVS2017.bat",
			"group": "build",
			"presentation": {
				"reveal": "always"
			},
			"problemMatcher": "$msCompile"
		},
		{
			"label": "re3",
			"type": "shell",
			"command": "D:/VS2017/MSBuild/15.0/Bin/MSBuild.exe",
			"args": [
				"${workspaceRoot}/build/re3.vcxproj",
				"/p:Configuration=Release;Platform=Win32;GenerateFullPaths=true",
				"/t:build"
			],
			"group": "build",
			"presentation": {
				"reveal": "always"
			},
			"problemMatcher": "$msCompile"
		}
	]
}